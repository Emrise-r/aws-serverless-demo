							"\r",
							"function makeid(length) {\r",
							"    let result           = '';\r",
							"    let characters       = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\r",
							"    let charactersLength = characters.length;\r",
							"    for ( let i = 0; i < length; i++ ) {\r",
							"      result += characters.charAt(Math.floor(Math.random() * \r",
							" charactersLength));\r",
							"   }\r",
							"   return result;\r",
							"}\r",
							"function randomString(length) {\r",
							"    let result = \"\";\r",
							"    for (let i = 0; i < length; i++) {\r",
							"            result += makeid(5) + \", \";\r",
							"    }\r",
							"    return result;\r",
							"}\r",
							"\r",
							"\r",
							"for (let i = 1; i < 4; i++) {\r",
							"    length = i + 5;\r",
							"    pm.environment.set(\"userId_\" + i, makeid(length))\r",
							"}\r",
							""
